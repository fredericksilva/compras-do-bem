.resultados
  .col.s4.m4.l4(style='padding: 3% 0 0; background: rgba(175, 215, 202, 0.22); border-radius: 10px;')
    .row
      .col.s12.m12.l12
        - var prof
        if (u.profile)
          - prof = 'circle'
        else
          - prof = 'z-depth-1'
        if (u.profile && u.profile.picture || u.avatar && u.avatar.img)
          img.responsive-img.profile(src=u.profile.picture || u.avatar.img, alt='', class=prof)
        else
          img.responsive-img.profile(src='/img/avatar-default.png', alt='', class=prof)
        // notice the "circle" class
      .col.s12.m12.l12
        h5(style='text-align: center; margin-bottom: 0;')
          if (u.profile)
            a(href='/user/' + u._id) #{u.profile.first_name.charAt(0).toUpperCase()}#{u.profile.first_name.slice(1)} #{u.profile.second_name.charAt(0).toUpperCase()}.
          else
            a(href='/servico/' + u.urlized) #{u.title}
        p(style='text-align: center; margin-top: 0; margin-bottom: 0;')
          | Rio de Janeiro - RJ
